{
  "name": "CustomSearch",
  "version": "2.1",
  "description": "Google Search enhanced - keyboard shortcuts and infinite scrolling.",
  "icons": {
    "48": "search48.png",
    "128": "search128.png"
  },
  "permissions": ["tabs"],
  "background_page": "background.html",
  "content_scripts": [
    {
      "matches": [
        "http://www.google.com/*",
        "http://www.google.ca/*",
        "http://www.google.com.au/*",
        "http://www.google.fr/*",
        "http://www.google.co.uk/*",
        "http://www.google.com.hk/*",
        "http://www.google.be/*",
        "http://www.google.ch/*",
        "http://www.google.ie/*",
        "http://www.google.es/*",
        "http://www.google.de/*",
        "http://www.google.it/*",
        "http://www.google.co.za/*",
        "http://www.google.co.jp/*",
        "http://www.google.gr/*"
      ],
      "js": ["search.js"]
    }
  ]
}
